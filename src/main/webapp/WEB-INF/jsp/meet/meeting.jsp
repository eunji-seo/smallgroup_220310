<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<div class="meetion-create">
	<div class="bg-white d-flex align-items-center p-3">
			<a href="/user/join_view"><img src="/static/image/arrow.png" width="50"></a>
			<div class="d-flex justify-content-center">
				<h3 class="favorite-subject">정모 개설</h3>
			</div>
	</div>    
	<div class="meeting-list mt-5">
		<div class="mb-3">
			<input type="text" id="" name="" class="form-control" placeholder="정모 이름">
		</div>
		<div class="d-flex mb-3">
			<div class="d-flex">	
				<img alt="" src="/static/image/calendar.png" width="35">	
				<input type="text" id="" name="" class="form-control col-8" placeholder="날짜">
			</div>
			<div class="d-flex">
				<img alt="" src="/static/image/person.png" width="35">	
				<input type="text" id="" name="" class="form-control col-8" placeholder="시간">
			</div>
		</div>
		<div class="d-flex mb-3">
			<img alt="" src="/static/image/location.png" width="35">	
			<input type="text" id="" name="" class="form-control" placeholder="장소 주소">
		</div>
		<div class="d-flex mb-3">
			<img alt="" src="/static/image/won.png" width="35">	
			<input type="text" id="" name="" class="form-control" placeholder="회비">
		</div>
		<div class="d-flex justify-content-between mb-3">
			<div>
				<img alt="" src="/static/image/person.png" width="40">
				<span>정원(5~10)</span>
			</div>
			<input type="text" id="" name="" class="form-control col-3" placeholder="정원 10">
		</div>
		<div>
		 <button type="button" class="btn btn-primary w-100" >정모 만들기</button>
		</div>
	</div>
</div>